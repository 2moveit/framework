<?xml version="1.0" encoding="utf-8" ?>
<!-- Este archivo contiene los parámetros necesarios para configurar el
	   cliente del Gestor de Salida.-->
<configuracion xmlns="http://signumsoftware.com/2007/GestorSalida">

	<!-- Especifica el directorio en el que se generan los arhivos temporales de informes -->
	<propiedad nombre="DirectorioTemporal" valor="D:\Temp" />

	<!-- Especifica si está habilitada la caché en memoria en el cliente para
	   los tipos comunes (se cargan en la primera petición al servidor)-->
	<propiedad nombre="CacheCliente" valor="true" />

</configuracion>
