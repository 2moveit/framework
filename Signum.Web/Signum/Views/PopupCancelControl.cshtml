@model Context

<div id="@Model.Compose("panelPopup")" class="sf-popup-control" data-prefix="@Model.ControlID" data-title="@((string)ViewData[ViewDataKeys.Title])">
    @{ 
        string partialView = (string)ViewData[ViewDataKeys.PartialViewName]; 
        if (partialView.HasText())
        {
            @Html.Partial(partialView, Model);
        }    
        else 
        {
            @ViewData[ViewDataKeys.CustomHtml]    
        }
    }
</div>
