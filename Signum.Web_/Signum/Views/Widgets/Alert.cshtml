@using Signum.Entities.Basics
@using (var tc = Html.TypeContext<AlertDN>())
{
    @Html.EntityLine(tc, n => n.Entity, el => el.ReadOnly = true)
    @Html.ValueLine(tc, n => n.AlertDate)
    @Html.ValueLine(tc, n => n.CheckDate)
    @Html.ValueLine(tc, n => n.Text, vl =>
{
    vl.ValueLineType = ValueLineType.TextArea;
});
} 